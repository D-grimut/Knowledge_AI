@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix vivo: <http://vivoweb.org/ontology/>.
@prefix dbo: <http://dbpedia.org/ontology/>.
@prefix uni: <http://uni.com/schema#>.
@prefix owl: <http://www.w3.org/2002/07/owl#> .

#-----------------------University------------------------------
uni:uni_dblink a rdf:Property;
    rdfs:domain dbo:University;
    rdfs:range xsd:string;
    rdfs:label "DBPedia Link"@en;
    rdfs:comment "University Link on DBPedia"@en.

uni:uni_wikilink a rdf:Property;
    rdfs:domain dbo:University;
    rdfs:label "Wikidata Link"@en;
    rdfs:comment "University Link on Wikidata"@en.

uni:name a rdfs:Property;
    rdfs:domain dbo:University;
    rdfs:range xsd:string;
    rdfs:label "name"@en;
    rdfs:comment "name of the entity (university) or one of its children"@en.

#-----------------------Course------------------------------
uni:Course a rdfs:Class;
    rdfs:subClassOf dbo:University;
    rdfs:label "Course"@en;
    rdfs:comment "Course class"@en.

uni:subject a rdf:Property;
    rdfs:domain uni:Course;
    rdfs:range xsd:string;
    rdfs:label "Subject"@en;
    rdfs:comment "Subject of the class"@en.

uni:ID a rdf:Property;
    rdfs:subPropertyOf vivo:identifier;
    rdfs:label "Course ID"@en;
    rdfs:comment "ID of the course"@en.

uni:credits a rdf:Property;
    rdfs:domain uni:Course;
    rdfs:range xsd:string;
    rdfs:label "Credits"@en;
    rdfs:comment "Number of credits of a class"@en.

uni:outline a rdf:Property;
    rdfs:domain uni:Course;
    rdfs:range xsd:string;
    rdfs:label "Outline"@en;
    rdfs:comment "Outline of a class"@en.

uni:offered_in a rdf:Property;
    rdfs:domain uni:Course;
    rdfs:range dbo:University;
    rdfs:label "Offered In"@en;
    rdfs:comment "University node where the course is offered in"@en.

# - Lectures RDFS - TODO

#-----------------------Topics------------------------------
uni:Topic a rdfs:Class;
    rdfs:label "Topic"@en;
    rdfs:comment "topics of the course"@en.

uni:topicName a rdf:Property;
    rdfs:domain uni:Topic;
    rdfs:range xsd:string;
    rdfs:label "Topic Name"@en;
    rdfs:comment "name of the topic"@en.

uni:provenance a rdf:Property;
    rdfs:domain uni:Topic;
    rdfs:range xsd:string;
    rdfs:label "Provenance"@en;
    rdfs:comment "a record of where the topic was identified as being covered in the course"@en.

uni:linkedTo a rdf:Property;
    rdfs:domain uni:Topic;
    rdfs:range xsd:anyURI;
    rdfs:label "Linked To"@en;
    rdfs:comment "link to a DBpedia and/or Wikidata"@en.

uni:topic_from a rdf:Property;
    rdfs:domain uni:Topic;
    rdfs:range uni:Course;
    rdfs:label "topic origin"@en;
    rdfs:comment "course(s) from where the topic is presented"@en.

uni:topic_offered a rdf:Property;
    rdfs:domain uni:Course;
    rdfs:range uni:Topic;
    rdfs:label "topic offered"@en;
    rdfs:comment "topic that is offered by a class"@en.

uni:topic_offered owl:inverseOf uni:topic_from.

#-----------------------Student------------------------------
dbo:student a rdfs:Class;
    rdfs:label "Student"@en;
    rdfs:comment "Student of an University"@en;
    rdfs:subClassOf foaf:Person.

uni:student_ID a rdf:Property;
    rdfs:domain dbo:student;
    rdfs:range xsd:string;
    rdfs:label "Student ID"@en;
    rdfs:comment "The unique identifier of a student"@en.

# cource_completed allows a student to have direct links with comepleted Classes.
# in the event the student retakes the class, we simply remove this link BUT keep the
# previous grade obtained by the student - we ubtated that grade when course is coompleted.

uni:completed_cource a rdf:Property;
    rdfs:domain dbo:student;
    rdfs:range uni:Course;
    rdfs:label "Cource Completed"@en;
    rdfs:comment "Cource ompleted by the student"@en.

uni:Grade a rdfs:Class;
    rdfs:label "Grade"@en;
    rdfs:comment "Letter Grade"@en.

uni:grade_obtained a rdf:Property;
    rdfs:domain dbo:student;
    rdfs:range uni:Grade;
    rdfs:label "Grade Obtained"@en;
    rdfs:comment "Grade Obtained by the student"@en.

uni:grade_obtained_in a rdf:Property;
    rdfs:domain uni:Grade;
    rdfs:range uni:Course;
    rdfs:label "Grade Obtained In"@en;
    rdfs:comment "Grade Obtained by the student in a specific course"@en.

#-----------------------Lectures------------------------------

#Used to get the lecture number of anything of type lecture
uni:LectureNumber rdf:type rdf:Property;
    rdfs:range uni:Lecture.

#Used to get the lecture name of anything of type lecture
uni:LectureName rdf:type rdf:Property;
    rdfs:range uni:Lecture.

#Used to link courses to lectures
uni:HasLecture rdf:type rdf:Property;
    rdfs:range uni:Course;
    rdfs:domain uni:Lecture.

uni:Lecture
    rdf:type rdfs:Class;
    rdfs:label "Lecture Class";
    rdfs:comment "The Class Type Lecture".

uni:Slides
    rdf:type rdfs:Class;
    rdfs:subClassOf uni:Lectures.

uni:Worksheets
    rdf:type rdfs:Class;
    rdfs:subClassOf uni:Lectures.

uni:Readings
    rdf:type rdfs:Class;
    rdfs:subClassOf uni:Lectures.

uni:OtherLectureMaterial
    rdf:type rdfs:Class;
    rdfs:subClassOf uni:Lectures.


